{
  "LambdaFunctionConfigurations": [
    {
      "Id": "DeepfakeDetectionTrigger",
      "LambdaFunctionArn": "arn:aws:lambda:us-east-1:YOUR_ACCOUNT_ID:function:deepfake-detection-s3-trigger",
      "Events": [
        "s3:ObjectCreated:*",
        "s3:ObjectRemoved:*"
      ],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "videos/"
            }
          ]
        }
      }
    },
    {
      "Id": "ResultsTrigger",
      "LambdaFunctionArn": "arn:aws:lambda:us-east-1:YOUR_ACCOUNT_ID:function:deepfake-detection-s3-trigger",
      "Events": [
        "s3:ObjectCreated:*"
      ],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "results/"
            }
          ]
        }
      }
    }
  ]
}

